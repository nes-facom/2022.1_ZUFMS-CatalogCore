{
  "$id": "https://inbio.ufms.br/zufms/zufmscore.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ZUFMS Darwin Core Subset",
  "type": "object",
  "required": [
    "scientificName",
    "artificial:section",
    "occurrenceID",
    "kingdom",
    "informationWithheld"
  ],
  "properties": {
    "artificial:section": {
      "title": "Seção",
      "description": "Grupo ao qual o registro faz parte",
      "examples": [
        "Amphibia",
        "Chelicerata",
        "Chiroptera",
        "Crustacea",
        "Diptera",
        "Fósseis",
        "Hemiptera",
        "Hymenoptera",
        "Mollusca",
        "Nematoda",
        "Odonata",
        "Orthoptera",
        "Peixes",
        "Reptilia",
        "Tecidos"
      ],
      "oneOf": [
        {
          "$zufmscore:initials": "ACA",
          "const": "Acanthocephala"
        },
        {
          "$zufmscore:initials": "AMP",
          "const": "Amphibia"
        },
        {
          "$zufmscore:initials": "ANN",
          "const": "Annelida"
        },
        {
          "$zufmscore:initials": "ARC",
          "const": "Archaeognatha"
        },
        {
          "$zufmscore:initials": "AVE",
          "const": "Aves"
        },
        {
          "$zufmscore:initials": "BLA",
          "const": "Blattodea"
        },
        {
          "$zufmscore:initials": "CHE",
          "const": "Chelicerata"
        },
        {
          "$zufmscore:initials": "CHI",
          "const": "Chiroptera"
        },
        {
          "$zufmscore:initials": "COB",
          "const": "Collembola"
        },
        {
          "$zufmscore:initials": "COL",
          "const": "Coleoptera"
        },
        {
          "$zufmscore:initials": "CRU",
          "const": "Crustacea"
        },
        {
          "$zufmscore:initials": "DER",
          "const": "Dermaptera"
        },
        {
          "$zufmscore:initials": "DIP",
          "const": "Diptera"
        },
        {
          "$zufmscore:initials": "EMB",
          "const": "Embioptera"
        },
        {
          "$zufmscore:initials": "EPH",
          "const": "Ephemeroptera"
        },
        {
          "$zufmscore:initials": "FOS",
          "const": "Fósseis"
        },
        {
          "$zufmscore:initials": "HEM",
          "const": "Hemiptera"
        },
        {
          "$zufmscore:initials": "HYM",
          "const": "Hymenoptera"
        },
        {
          "$zufmscore:initials": "ISO",
          "const": "Isoptera"
        },
        {
          "$zufmscore:initials": "LEP",
          "const": "Lepidoptera"
        },
        {
          "$zufmscore:initials": "MAN",
          "const": "Mantodea"
        },
        {
          "$zufmscore:initials": "MEC",
          "const": "Mecoptera"
        },
        {
          "$zufmscore:initials": "MEG",
          "const": "Megaloptera"
        },
        {
          "$zufmscore:initials": "MID",
          "const": "Mídias Digitais"
        },
        {
          "$zufmscore:initials": "MNV",
          "const": "Mamíferos Não-Voadores"
        },
        {
          "$zufmscore:initials": "MOL",
          "const": "Mollusca"
        },
        {
          "$zufmscore:initials": "MYR",
          "const": "Myriapoda"
        },
        {
          "$zufmscore:initials": "NEM",
          "const": "Nematoda"
        },
        {
          "$zufmscore:initials": "NEU",
          "const": "Neuroptera"
        },
        {
          "$zufmscore:initials": "ODO",
          "const": "Odonata"
        },
        {
          "$zufmscore:initials": "ORT",
          "const": "Orthoptera"
        },
        {
          "$zufmscore:initials": "PHA",
          "const": "Phasmatodea"
        },
        {
          "$zufmscore:initials": "PIS",
          "const": "Pisces"
        },
        {
          "$zufmscore:initials": "PLA",
          "const": "Platyhelminthes"
        },
        {
          "$zufmscore:initials": "PLE",
          "const": "Plecoptera"
        },
        {
          "$zufmscore:initials": "PSO",
          "const": "Psocodea"
        },
        {
          "$zufmscore:initials": "REP",
          "const": "Reptilia"
        },
        {
          "$zufmscore:initials": "SIP",
          "const": "Siphonaptera"
        },
        {
          "$zufmscore:initials": "TEC",
          "const": "Tecidos"
        },
        {
          "$zufmscore:initials": "THY",
          "const": "Thysanoptera"
        },
        {
          "$zufmscore:initials": "TRI",
          "const": "Trichoptera"
        },
        {
          "$zufmscore:initials": "ZYG",
          "const": "Zygentoma"
        }
      ],
      "$zufmscore:autocomplete": true,
      "$zufmscore:termclass": "zufmscore:management"
    },
    "occurrenceID": {
      "type": "string",
      "description": "identidade do exemplar, utilizado para evitar duplicações",
      "pattern": "^br:UFMS:ZUFMS:ZUFMS-[A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][0-9]$",
      "title": "ID da Ocorrência",
      "$zufmscore:dynamic": "occurenceId",
      "examples": [
        "br:UFMS:ZUFMS-CRU00002",
        "br:UFMS:ZUFMS-AMP00002"
      ],
      "$zufmscore:termclass": "occurrence"
    },
    "otherCatalogNumbers": {
      "type": "string",
      "description": "quando proveniente de outra coleção",
      "title": "Número de Catálogo Anterior",
      "examples": [
        "AMP01765",
        "AMP716"
      ],
      "$zufmscore:termclass": "occurrence"
    },
    "recordNumber": {
      "type": "string",
      "description": "número da etiqueta de campo, associada ao coletor",
      "title": "Número da Etiqueta de Campo",
      "examples": [
        "CH381",
        "SF454"
      ],
      "$zufmscore:repeat": true,
      "default": "",
      "$zufmscore:termclass": "occurrence"
    },
    "associatedOccurrences": {
      "type": "string",
      "description": "espécime tombado que tenha relação com o registro",
      "title": "Item Relacionado ao Catálogo",
      "examples": [
        "ZUFMS-LEP00001|ZUFMS-COL00002",
        "ZUFMS-AMP00001|ZUFMS-TEC00002"
      ],
      "$zufmscore:termclass": "organism"
    },
    "relationshipOfResource": {
      "type": "string",
      "description": "informar a relação com o Item Relacionado ao Catálogo",
      "title": "Tipo de Relacionamento",
      "default": "nenhum",
      "examples": [
        "nenhum"
      ],
      "$zufmscore:termclass": "resourceRelationship"
    },
    "taxonRank": {
      "type": "string",
      "description": "informa até que nível taxonômico foi identificado",
      "title": "Nível do Táxon",
      "$zufmscore:autocomplete": true,
      "examples": [
        "subespécie"
      ],
      "$zufmscore:termclass": "taxon"
    },
    "scientificName": {
      "type": "string",
      "description": "similar ao Nome Científico Aceito, mas aqui utilizamos sem autoria",
      "title": "Nome Científico",
      "examples": [
        "Lysapsus limellum",
        "Leptodactylus labyrinthicus",
        "Dendropsophus minutus"
      ],
      "$zufmscore:termclass": "taxon"
    },
    "taxonomicStatus": {
      "type": "string",
      "description": "informa a condição taxonômica:",
      "title": "Status Taxonômico",
      "$zufmscore:autocomplete": true,
      "examples": [
        "em descrição",
        "inválido",
        "mal aplicado",
        "sinônimo homotípico",
        "aceito"
      ],
      "$zufmscore:termclass": "taxon"
    },
    "originalNameUsage": {
      "type": "string",
      "description": "se for um táxon com problemas, informar o original",
      "title": "Nome Original Usado",
      "examples": [
        "Dilocarcinus pagei Stimpson, 1861"
      ],
      "$zufmscore:termclass": "taxon"
    },
    "vernacularName": {
      "type": "string",
      "description": "nome popular hifenizado",
      "title": "Nome Vulgar",
      "$zufmscore:autocomplete": true,
      "examples": [
        "caramujo-d'água"
      ],
      "$zufmscore:termclass": "taxon"
    },
    "typeStatus": {
      "type": "string",
      "description": "informa se é tipo nomenclatural ou apenas voucher, se necessário concatenar com \";\"",
      "title": "Tipo Nomenclatural",
      "$zufmscore:autocomplete": true,
      "$zufmscore:repeat": true,
      "examples": [
        "Holótipo de Pinus abies | Holótipo de Picea abies",
        "Holótipo"
      ],
      "$zufmscore:termclass": "identification"
    },
    "identifiedBy": {
      "type": "string",
      "description": "o sobrenome e nome abreviado em caixa alta segundo o exemplo de quem identificou o material",
      "title": "Determinado Por",
      "$zufmscore:repeat": true,
      "examples": [
        "GRACIOLLI, G.",
        "James L. Patton",
        "Theodore Pappenfuss | Robert Macey"
      ],
      "$zufmscore:termclass": "identification"
    },
    "dateIdentified": {
      "type": "string",
      "description": "data da determsinação no formato aaaa-mm-dd",
      "pattern": "^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))$|^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012]))$|^([1-2][0-9][0-9][0-9])$",
      "title": "Data da Determinação",
      "$zufmscore:repeat": true,
      "examples": [
        "2012-03-27",
        "2012-12-30",
        "2022-02-20"
      ],
      "$zufmscore:termclass": "identification"
    },
    "identificationQualifier": {
      "type": "string",
      "description": "quando não se sabe ao certo a identificação ",
      "title": "Imprecisão",
      "$zufmscore:autocomplete": true,
      "$zufmscore:repeat": true,
      "examples": [
        "cf.",
        "aff.",
        "sp.",
        "gr.",
        "fo."
      ],
      "$zufmscore:termclass": "identification"
    },
    "recordedBy": {
      "type": "string",
      "description": "nome do coletor abreviado seguindo o exemplo \"SOBRENOME, N.\"",
      "title": "Coletor",
      "$zufmscore:repeat": true,
      "default": "",
      "$zufmscore:termclass": "occurrence"
    },
    "day": {
      "type": "integer",
      "description": "dia em algarismo arábico",
      "title": "Dia da Coleta",
      "$zufmscore:repeat": true,
      "examples": [
        1,
        23,
        10
      ],
      "$zufmscore:termclass": "event"
    },
    "month": {
      "type": "integer",
      "description": "mês em algarismo arábico",
      "title": "Mês da Coleta",
      "$zufmscore:repeat": true,
      "examples": [
        12,
        7,
        2
      ],
      "$zufmscore:termclass": "event"
    },
    "year": {
      "type": "integer",
      "description": "ano em algarismo arábico",
      "title": "Ano da Coleta",
      "$zufmscore:repeat": true,
      "examples": [
        2004,
        1987,
        2013
      ],
      "$zufmscore:termclass": "event"
    },
    "eventDate": {
      "type": "string",
      "pattern": "(^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))$|^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012]))$|^(([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))((\\/)([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))))$|^([1-2][0-9][0-9][0-9])$|^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])(\\/)[1-2][0-9][0-9][0-9]-(0?[1-9]|1[012]))$)|(^[1-2][0-9][0-9][0-9]\\/[1-2][0-9][0-9][0-9]$)",
      "description": "data (aaaa-mm-dd) ou intervalo (aaaa-mm-dd/aaaa-mm-dd), mas se não houver todas informações informar apenas mês (aaaa-mm) ou ano (aaaa)",
      "title": "Data/Intervalo do Evento",
      "examples": [
        "2010-06-30",
        "2010-12-01/2010-12-31"
      ],
      "$zufmscore:termclass": "event"
    },
    "eventTime": {
      "type": "string",
      "description": "hora única (hh:mm) ou intervalo (hh:mm/hh:mm)",
      "title": "Hora/Intervalo do Evento",
      "examples": [
        "19:00/21:00",
        "22:23"
      ],
      "$zufmscore:termclass": "event"
    },
    "fieldNumber": {
      "type": "string",
      "description": "",
      "title": "Número do Campo",
      "examples": [
        "Pitfall0001Seca"
      ],
      "$zufmscore:termclass": "event"
    },
    "samplingProtocol": {
      "type": "string",
      "description": "descrição sucinta do método de coleta",
      "title": "Método de Coleta",
      "$zufmscore:repeat": true,
      "examples": [
        "busca ativa"
      ],
      "$zufmscore:termclass": "event"
    },
    "habitat": {
      "type": "string",
      "description": "pode ser bioma, ecorregião que caracterize o ambiente natural do espécime",
      "title": "Habitat",
      "$zufmscore:repeat": true,
      "examples": [
        "Pantanal"
      ],
      "$zufmscore:termclass": "event"
    },
    "eventRemarks": {
      "type": "string",
      "description": "comentários sobre o evento",
      "title": "Observações do Evento",
      "examples": [
        "após das recentes chuvas o rio está quase na fase de inundação"
      ],
      "$zufmscore:termclass": "event"
    },
    "fieldNotes": {
      "type": "string",
      "description": "pode ser um indicador da existencia de, b) uma referência a (publicação, link), ou c) texto das notas tomadas no campo sobre o evento",
      "title": "Anotações de Campo",
      "examples": [
        "notas disponíveis na Biblioteca Central"
      ],
      "$zufmscore:termclass": "event"
    },
    "country": {
      "type": "string",
      "description": "nome do país",
      "title": "País",
      "default": "Brasil",
      "$zufmscore:repeat": true,
      "examples": [
        "Brasil",
        "Paraguai",
        "Bolívia"
      ],
      "$zufmscore:termclass": "location"
    },
    "stateProvince": {
      "type": "string",
      "description": "escrito por extenso, utilizar região quando são áreas de divisa entre estados",
      "title": "Estado/Província/Região",
      "default": "Mato Grosso do Sul",
      "$zufmscore:repeat": true,
      "examples": [
        "Mato Grosso do Sul",
        "Paraná",
        "São Paulo"
      ],
      "$zufmscore:termclass": "location"
    },
    "county": {
      "type": "string",
      "description": "o nome do município válido",
      "title": "Município",
      "$zufmscore:repeat": true,
      "examples": [
        "Campo Grande",
        "Corguinho",
        "Três Lagoas"
      ],
      "$zufmscore:termclass": "location"
    },
    "municipality": {
      "type": "string",
      "description": "quando há necessidade de informar distrito ou sub-região (ex.: Pantanal da Nhecolândia)",
      "title": "Sub-Região/Distrito",
      "$zufmscore:repeat": true,
      "examples": [
        "Taboco",
        "Pedro Gomes"
      ],
      "$zufmscore:termclass": "location"
    },
    "locality": {
      "type": "string",
      "description": "a localidade abaixo de município ou distrito",
      "title": "Localidade",
      "$zufmscore:repeat": true,
      "examples": [
        "Fazenda Nhumirim",
        "Fazenda Dois Irmãos"
      ],
      "$zufmscore:termclass": "location"
    },
    "decimalLatitude": {
      "type": "number",
      "pattern": "^((-[0-8]?[0-9].\\d\\d\\d\\d\\d\\d$)|([0-8]?[0-9].\\d\\d\\d\\d\\d\\d$)|(-?[9][0].[0][0][0][0][0][0]$))$",
      "minimum": -90,
      "maximum": 90,
      "description": "coordenada em formato decimal, a célula deve estar formatada em texto",
      "title": "Latitude Decimal",
      "$zufmscore:repeat": true,
      "examples": [
        -45.56732,
        -30.24545,
        -22.65665
      ],
      "$zufmscore:termclass": "location"
    },
    "decimalLongitude": {
      "type": "number",
      "minimum": -180,
      "maximum": 180,
      "pattern": "^(-?[1]?[0-7]?[0-9].\\d\\d\\d\\d\\d\\d)$|^(-?[1][8][0].[0][0][0][0][0][0])$|^(-?[0-9]?[0-9].\\d\\d\\d\\d\\d\\d)$",
      "description": "coordenada em formato decimal, a célula deve estar formatada em texto",
      "title": "Longitude Decimal",
      "$zufmscore:repeat": true,
      "examples": [
        -45.56732,
        -30.24545,
        -22.65665
      ],
      "$zufmscore:termclass": "location"
    },
    "coordinatePrecision": {
      "type": "number",
      "description": "precisão em metros",
      "title": "Precisão da Coordenada (m)",
      "default": 0,
      "$zufmscore:repeat": true,
      "examples": [
        10,
        24,
        2
      ],
      "$zufmscore:termclass": "location"
    },
    "geodeticDatum": {
      "type": "string",
      "description": "informar somente se decimal",
      "title": "Datum",
      "$zufmscore:autocomplete": true,
      "$zufmscore:repeat": true,
      "$zufmscore:highestoccurrence": true,
      "examples": [
        "WGS84",
        "NAD27",
        "Campo Inchauspe"
      ],
      "$zufmscore:termclass": "location"
    },
    "footprintWKT": {
      "type": "string",
      "description": "quando não se sabe a coordenada exata; acesse http://boundingbox.klokantech.com/ para estimar o quadrante",
      "title": "Retângulo Envolvente",
      "$zufmscore:repeat": true,
      "examples": [
        "GPS: 019°18'00\"S--021°39'00\"S/056°22'00\"W--053°34'00\"W",
        "POLYGON((-54.9253 -20.1652, -53.5997 -20.1652, -53.5997 -21.5845, -54.9253 -21.5845, -54.9253 -20.1652))",
        "POLYGON((-55.0794 -19.8, -54.6514 -19.8, -54.6514 -20.1022, -55.0794 -20.1022, -55.0794 -19.8))"
      ],
      "$zufmscore:termclass": "location"
    },
    "verbatimLocality": {
      "type": "string",
      "description": "utilizar quando se trata de organismos aquáticos",
      "title": "Bacia Hidrográfica",
      "default": "Brasil",
      "$zufmscore:repeat": true,
      "examples": [
        "Bacia do Paraguai",
        "Bacia Hidrográfica Amazônica",
        "Bacia Hidrográfica do Paraná "
      ],
      "$zufmscore:termclass": "location"
    },
    "minimumElevationInMeters": {
      "type": "number",
      "description": "limite inferior do intervalo da elevação",
      "title": "Altitude Mínima (m)",
      "$zufmscore:repeat": true,
      "examples": [
        100,
        517,
        524
      ],
      "$zufmscore:termclass": "location"
    },
    "maximumElevationInMeters": {
      "type": "number",
      "description": "limite superior do intervalo da elevação",
      "title": "Altitude Máxima (m)",
      "$zufmscore:repeat": true,
      "examples": [
        100,
        517,
        524
      ],
      "$zufmscore:termclass": "location"
    },
    "waterBody": {
      "type": "string",
      "description": "o nome do corpo d'água lótico/lêntico onde foi coletado; mais detalhes acesse http://www.getty.edu/research/tools/vocabularies/tgn/",
      "title": "Corpo D'Água",
      "$zufmscore:repeat": true,
      "examples": [
        "Rio Salobra",
        "Rio Paraguai",
        "Rio Aquidauna"
      ],
      "$zufmscore:termclass": "location"
    },
    "minimumDepthInMeters": {
      "type": "number",
      "description": "a menor profundidade de um intervalo de profundidades abaixo da superfície do local, em metros",
      "title": "Profundidade Mínima (m)",
      "$zufmscore:repeat": true,
      "examples": [
        100,
        30,
        15
      ],
      "$zufmscore:termclass": "location"
    },
    "maximumDepthInMeters": {
      "type": "number",
      "description": "a maior profundidade de um intervalo de profundidades abaixo da superfície do local, em metros",
      "title": "Profundidade Máxima (m)",
      "$zufmscore:repeat": true,
      "examples": [
        100,
        234,
        15
      ],
      "$zufmscore:termclass": "location"
    },
    "locationRemarks": {
      "type": "string",
      "description": "comentários sobre o local onde foi coletado",
      "title": "Observações do Local",
      "$zufmscore:repeat": true,
      "examples": [
        "debaixo de água desde 2005",
        "Mata Fechada",
        "Pasto Verdejante"
      ],
      "$zufmscore:termclass": "location"
    },
    "individualCount": {
      "type": [
        "string",
        "integer"
      ],
      "description": "se for em lote, informar a quantidade",
      "title": "Número de Indivíduos",
      "default": "1",
      "examples": [
        147,
        "1"
      ],
      "$zufmscore:termclass": "occurrence"
    },
    "sex": {
      "type": "string",
      "description": "Campo informado com vocabulário",
