{
    "$id": "https://inbio.ufms.br/zufms/zufmscore.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "ZUFMS Darwin Core Subset",
    "type": "object",
    "required":[],
    "properties": {
        "artificial:section": {
            "title": "Seção",
            "description": "Grupo ao qual o registro faz parte",
            "examples": [
                "Amphibia",
                "Chelicerata",
                "Chiroptera",
                "Crustacea",
                "Diptera",
                "Fósseis",
                "Hemiptera",
                "Hymenoptera",
                "Mollusca",
                "Nematoda",
                "Odonata",
                "Orthoptera",
                "Peixes",
                "Reptilia",
                "Tecidos"
            ],
            "oneOf": [
                {
                    "$zufmscore:initials": "ACA",
                    "const": "Acanthocephala"
                },
                {
                    "$zufmscore:initials": "AMP",
                    "const": "Amphibia"
                },
                {
                    "$zufmscore:initials": "ANN",
                    "const": "Annelida"
                },
                {
                    "$zufmscore:initials": "ARC",
                    "const": "Archaeognatha"
                },
                {
                    "$zufmscore:initials": "AVE",
                    "const": "Aves"
                },
                {
                    "$zufmscore:initials": "BLA",
                    "const": "Blattodea"
                },
                {
                    "$zufmscore:initials": "CHE",
                    "const": "Chelicerata"
                },
                {
                    "$zufmscore:initials": "CHI",
                    "const": "Chiroptera"
                },
                {
                    "$zufmscore:initials": "COB",
                    "const": "Collembola"
                },
                {
                    "$zufmscore:initials": "COL",
                    "const": "Coleoptera"
                },
                {
                    "$zufmscore:initials": "CRU",
                    "const": "Crustacea"
                },
                {
                    "$zufmscore:initials": "DER",
                    "const": "Dermaptera"
                },
                {
                    "$zufmscore:initials": "DIP",
                    "const": "Diptera"
                },
                {
                    "$zufmscore:initials": "EMB",
                    "const": "Embioptera"
                },
                {
                    "$zufmscore:initials": "EPH",
                    "const": "Ephemeroptera"
                },
                {
                    "$zufmscore:initials": "FOS",
                    "const": "Fósseis"
                },
                {
                    "$zufmscore:initials": "HEM",
                    "const": "Hemiptera"
                },
                {
                    "$zufmscore:initials": "HYM",
                    "const": "Hymenoptera"
                },
                {
                    "$zufmscore:initials": "ISO",
                    "const": "Isoptera"
                },
                {
                    "$zufmscore:initials": "LEP",
                    "const": "Lepidoptera"
                },
                {
                    "$zufmscore:initials": "MAN",
                    "const": "Mantodea"
                },
                {
                    "$zufmscore:initials": "MEC",
                    "const": "Mecoptera"
                },
                {
                    "$zufmscore:initials": "MEG",
                    "const": "Megaloptera"
                },
                {
                    "$zufmscore:initials": "MID",
                    "const": "Mídias Digitais"
                },
                {
                    "$zufmscore:initials": "MNV",
                    "const": "Mamíferos Não-Voadores"
                },
                {
                    "$zufmscore:initials": "MOL",
                    "const": "Mollusca"
                },
                {
                    "$zufmscore:initials": "MYR",
                    "const": "Myriapoda"
                },
                {
                    "$zufmscore:initials": "NEM",
                    "const": "Nematoda"
                },
                {
                    "$zufmscore:initials": "NEU",
                    "const": "Neuroptera"
                },
                {
                    "$zufmscore:initials": "ODO",
                    "const": "Odonata"
                },
                {
                    "$zufmscore:initials": "ORT",
                    "const": "Orthoptera"
                },
                {
                    "$zufmscore:initials": "PHA",
                    "const": "Phasmatodea"
                },
                {
                    "$zufmscore:initials": "PIS",
                    "const": "Pisces"
                },
                {
                    "$zufmscore:initials": "PLA",
                    "const": "Platyhelminthes"
                },
                {
                    "$zufmscore:initials": "PLE",
                    "const": "Plecoptera"
                },
                {
                    "$zufmscore:initials": "PSO",
                    "const": "Psocodea"
                },
                {
                    "$zufmscore:initials": "REP",
                    "const": "Reptilia"
                },
                {
                    "$zufmscore:initials": "SIP",
                    "const": "Siphonaptera"
                },
                {
                    "$zufmscore:initials": "TEC",
                    "const": "Tecidos"
                },
                {
                    "$zufmscore:initials": "THY",
                    "const": "Thysanoptera"
                },
                {
                    "$zufmscore:initials": "TRI",
                    "const": "Trichoptera"
                },
                {
                    "$zufmscore:initials": "ZYG",
                    "const": "Zygentoma"
                }
            ],
            "$zufmscore:autocomplete": true,
            "$zufmscore:termclass": "zufmscore:management"
        },
        "dcterms:modified": {
            "type": "string",
            "description": "data que o registro foi criado ou alterado, no padrão AAAA-MM-DD",
            "title": "Data da Última Modificação",
            "format": "date",
            "examples": [
                "2017-08-28",
                "2022-12-02"
            ],
            "$zufmscore:dynamic": "currentdate",
            "$zufmscore:section": "recordLevel"
        },
        "informationWithheld": {
            "type": "string",
            "description": "informação que falta em uma das colunas e deve ser procurada",
            "title": "Informação Retida",
            "examples": [
                "verificar amostra de tecido"
            ],
            "default": "nenhuma",
            "$zufmscore:section": "recordLevel"
        },
        "basisOfRecord": {
            "type": "string",
            "description": "PreservedSpecimen: um recurso que descreve um espécime preservado. LivingSpecimen: um recurso que descreve um espécime vivo. HumanObservation: um recurso que descreve uma observação feita por uma ou mais pessoas. MachineObservation: um recurso que descreve uma observação feita por máquina. Sample: um recurso que descreve os resultados físicos de uma amostragem.",
            "title": "Base do Registro",
            "examples": [
                "PreservedSpecimen",
                "LivingSpecimen",
                "HumanObservation",
                "MachineObservation",
                "Sample"
            ],
            "$zufmscore:autocomplete": true,
            "$zufmscore:highestoccurrence": true,
            "$zufmscore:section": "recordLevel"
        },
        "institutionCode": {
            "type": "string",
            "description": "sigla da instituição",
            "title": "Código da Instituição",
            "default": "UFMS",
            "examples": [
                "UFMS"
            ],
            "$zufmscore:section": "recordLevel"
        },
        "collectionCode": {
            "type": "string",
            "description": "sigla da coleção",
            "title": "Código da Coleção",
            "default": "ZUFMS",
            "examples": [
                "ZUFMS"
            ],
            "$zufmscore:section": "recordLevel"
        },
        "dcterms:bibliographicCitation": {
            "type": "string",
            "description": "citação do artigo em que este exemplar foi utilizado, concatenar com \";\"",
            "title": "Citação Bibliográfica",
            "examples": [
                "Oliver P. Pearson. 1985. Los tuco-tucos (genera Ctenomys) de los Parques Nacionales Lanin y Nahuel Huapi, Argentina Historia Natural, 5(37):337-343."
            ],

            "$zufmscore:repeat": true,
            "$zufmscore:section": "recordLevel"
        },
        "datasetName": {
            "type": "string",
            "description": "o título do projeto no qual houve envolvimento de coletas",
            "title": "Nome do Conjunto de Dados",
            "examples": [
                "Projeto Jauru",
                "AQUARAP - Programa de Avaliação Rápida de Ambientes Aquáticos"
            ],

            "$zufmscore:repeat": true,
            "$zufmscore:section": "recordLevel"
        },
        "artificial:shelfLocation": {
            "type": "string",
            "description": "local numerado onde o exemplar está armazenado (utilize ALT+0150 para inserir a \"meia-risca\" se forem várias prateleiras)",
            "title": "Localização na Coleção (Prateleira)",
            "examples": [
                "P001",
                "P020"
            ],

            "$zufmscore:repeat": true,
            "$zufmscore:section": "zufmscore:management"
        },
        "artificial:flaskLocation": {
            "type": "string",
            "description": "acronímia da seção (ex.: AMP) que agrupa alfabeticamente vários tombos, desde que sejam mesmo táxon, local e ano de coleta",
            "title": "Localização na Coleção (Frasco)",
            "examples": [
                "AMPAA",
                "AMPVJ"
            ],

            "$zufmscore:repeat": true,
            "$zufmscore:section": "zufmscore:management"
        },
        "artificial:shippingGuide": {
            "type": "string",
            "description": "exemplar não disponível",
            "title": "Guia de Remessa",
            "examples": [
                "GR07/2017",
                "GR01/2022"
            ],

            "$zufmscore:section": "zufmscore:management"
        },
        "catalogNumber": {
            "type": "string",
            "description": "número de tombo do exemplar",
            "pattern": "^ZUFMS-[A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][0-9]$",
            "title": "Número de Catálogo",
            "$zufmscore:section": "occurence",
            "$zufmscore:dynamic": "sectionIndex",
            "examples": [
                "ZUFMS-AMP00001",
                "ZUFMS-BLA00001"
            ]
        },
        "otherCatalogNumbers": {
            "type": "string",
            "description": "quando proveniente de outra coleção",
            "title": "Número de Catálogo Anterior",
            "examples": [
                "AMP01765",
                "AMP716"
            ],

            "$zufmscore:section": "occurence"
        },
        "recordedBy": {
            "type": "string",
            "description": "nome do coletor abreviado seguindo o exemplo \"SOBRENOME, N.\"",
            "title": "Coletor",
            "$zufmscore:section": "occurence",
            "$zufmscore:repeat": true,
            "default": ""
        },
        "recordNumber": {
            "type": "string",
            "description": "número da etiqueta de campo, associada ao coletor",
            "title": "Número da Etiqueta de Campo",
            "examples": [
                "CH381",
                "SF454"
            ],
            "$zufmscore:section": "occurence",
            "$zufmscore:repeat": true,
            "default": ""
        },
        "preparations": {
            "type": "string",
            "description": "tipo de preparação concatenado com \";\"",
            "title": "Tipo de Preparação",
            "$zufmscore:section": "occurence",
            "$zufmscore:autocomplete": true,
            "$zufmscore:highestoccurrence": true,
            "examples": [
                "animal inteiro (ETOH)",
                "tecido (EDTA)"
            ]
        },
        "individualCount": {
            "type": ["string","integer"],
            "description": "se for em lote, informar a quantidade",
            "title": "Número de Indivíduos",
            "$zufmscore:section": "occurence",
            "default": "1",
            "examples": [
                147,
                "1"
            ]
        },
        "sex": {
            "type": "string",
            "description": "Campo informado com vocabulário",
            "title": "Sexo",
            "$zufmscore:autocomplete": true,
            "$zufmscore:highestoccurrence": true,
            "$zufmscore:section": "occurence",
            "examples": [
                "irreconhecível",
                "indeterminado",
                "macho",
                "fêmea",
                "hermafrodita",
                "ginandromorfo"
            ]
        },
        "lifeStage": {
            "type": "string",
            "description": "o estágio de vida do especime no momento do registro",
            "title": "Fase da Vida",
            "$zufmscore:section": "occurence",
            "$zufmscore:autocomplete": true,
            "$zufmscore:repeat": true,
            "examples": [
                "zigoto",
                "embrião",
                "adulto",
                "juvenil",
                "ovo",
                "larva",
                "pupa"
            ]
        },
        "reproductiveCondition": {
            "type": "string",
            "description": "informa que fase reprodutiva se encontra",
            "title": "Condição Reprodutiva",
            "$zufmscore:section": "occurence",
            "$zufmscore:autocomplete": true,
            "$zufmscore:repeat": true,
            "examples": [
                "não reprodutivo",
                "grávida",
                "não informado"
            ]
        },
        "establishmentMeans": {
            "type": "string",
            "description": "informa a real origem com vocabulário",
            "title": "Modo de Estabelecimento",
            "$zufmscore:section": "occurence",
            "$zufmscore:autocomplete": true,
            "$zufmscore:repeat": true,
            "examples": [
                "selvagem",
                "nativa",
                "introduzida",
                "naturalizada",
                "manejada",
                "incerto",
                "selvagem"
            ]
        },
        "behavior": {
            "type": "string",
            "description": "a ação que o espécime realizava quando ainda vivo",
            "title": "Comportamento",
            "$zufmscore:section": "occurence",
            "$zufmscore:autocomplete": true,
            "default": "não informado",
            "examples": [
                "selvagem",
                "forrageando",
                "correndo",
                "não informado"
            ]
        },
        "occurrenceRemarks": {
            "type": "string",
            "description": "observação ou comentário do espécime na hora da coleta",
            "title": "Observações de Ocorrência",
            "$zufmscore:section": "occurence",
            "default": "não informado",
            "examples": [
                "espécime encontrado morto na estrada"
            ]
        },
        "disposition": {
            "type": "string",
            "description": "informação do registro na coleção",
            "title": "Disposição",
            "$zufmscore:section": "occurence",
            "$zufmscore:autocomplete": true,
            "default": "verificar",
            "examples": [
                "em coleção"
            ]
        },
        "associatedReferences": {
            "type": "string",
            "description": "bibliografia relacionada ou direta",
            "title": "Referências Associadas",
            "$zufmscore:section": "occurence",
            "$zufmscore:repeat": true,

            "examples": [
                "GALLARDO, Fabiana E.; RECHE, Vanina A.; MONTEIRO, Ricardo Ferreira. New data on Marthiella Buffington (Hymenoptera, Cynipoidea, Figitidae), with description of a new species. Rev. Bras. entomol., São Paulo ,  v. 58, n. 2, jun.  2014"
            ]
        },
        "associatedMedia": {
            "type": "string",
            "description": "o link ou pasta onde está a mídia associada",
            "title": "Mídia Associada",
            "$zufmscore:section": "occurence",

            "examples": [
                "inbio.br/photo.jpg"
            ]
        },
        "occurrenceID": {
            "type": "string",
            "description": "identidade do exemplar, utilizado para evitar duplicações",
            "pattern": "^br:UFMS:ZUFMS:ZUFMS-[A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][0-9]$",
            "title": "ID da Ocorrência",
            "$zufmscore:section": "occurence",
            "$zufmscore:dynamic": "occurenceId",
            "examples": [
                "br:UFMS:ZUFMS-CRU00002",
                "br:UFMS:ZUFMS-AMP00002"
            ]
        },
        "associatedOccurrences": {
            "type": "string",
            "description": "espécime tombado que tenha relação com o registro",
            "title": "Item Relacionado ao Catálogo",
            "$zufmscore:section": "occurence",

            "examples": [
                "ZUFMS-LEP00001|ZUFMS-COL00002",
                "ZUFMS-AMP00001|ZUFMS-TEC00002"
            ]
        },
        "previousIdentifications": {
            "type": "string",
            "description": "identificações anteriores, informando táxon, determinador, data e método utilizado",
            "title": "Identificações Anteriores",
            "$zufmscore:section": "occurence",

            "examples": [
                "Anthus sp., ID de campo por G. Iglesias; Anthus correndera, ID do experto por C. Cicero 2009-02-12 baseado em morfologia"
            ]
        },
        "fieldNumber": {
            "type": "string",
            "description": "",
            "title": "Número do Campo",
            "$zufmscore:section": "event",
            "examples": [
                "Pitfall0001Seca"
            ]
        },
        "day": {
            "type": "integer",
            "description": "dia em algarismo arábico",
            "title": "Dia da Coleta",
            "$zufmscore:section": "event",

            "$zufmscore:repeat": true,
            "examples": [
                1,
                23,
                10
            ]
        },
        "month": {
            "type": "integer",
            "description": "mês em algarismo arábico",
            "title": "Mês da Coleta",
            "$zufmscore:section": "event",

            "$zufmscore:repeat": true,
            "examples": [
                12,
                7,
                2
            ]
        },
        "year": {
            "type": "integer",
            "description": "ano em algarismo arábico",
            "title": "Ano da Coleta",
            "$zufmscore:section": "event",

            "$zufmscore:repeat": true,
            "examples": [
                2004,
                1987,
                2013
            ]
        },
        "eventTime": {
            "type": "string",
            "description": "hora única (hh:mm) ou intervalo (hh:mm/hh:mm)",
            "title": "Hora/Intervalo do Evento",
            "$zufmscore:section": "event",

            "examples": [
                "19:00/21:00",
                "22:23"
            ]
        },
        "eventDate": {
            "type": "string",
            "pattern": "(^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))$|^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012]))$|^(([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))((\\\/)([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])\\-((0[1-9])|([1-2][0-9])|(3[01])))))$|^([1-2][0-9][0-9][0-9])$|^([1-2][0-9][0-9][0-9]-(0?[1-9]|1[012])(\\\/)[1-2][0-9][0-9][0-9]-(0?[1-9]|1[012]))$)|(^[1-2][0-9][0-9][0-9]\\\/[1-2][0-9][0-9][0-9]$)",
            "description": "data (aaaa-mm-dd) ou intervalo (aaaa-mm-dd/aaaa-mm-dd), mas se não houver todas informações informar apenas mês (aaaa-mm) ou ano (aaaa)",
            "title": "Data/Intervalo do Evento",
            "$zufmscore:section": "event",
            "examples": [
                "2010-06-30",
                "2010-12-01/2010-12-31"
            ]
        },
        "verbatimEventDate": {
            "type": "string",

            "description": "data clássica com mês em formato romano",
            "title": "Data Coleta Textual",
            "$zufmscore:section": "event",
            "examples": [
                "17.IV.2015",
                "17.XI.2015"
            ]
        },
        "samplingProtocol": {
            "type": "string",
            "description": "descrição sucinta do método de coleta",
            "title": "Método de Coleta",
            "$zufmscore:section": "event",

            "$zufmscore:repeat": true,
            "examples": [
                "busca ativa"
            ]
        },
        "habitat": {
            "type": "string",
            "description": "pode ser bioma, ecorregião que caracterize o ambiente natural do espécime",
            "title": "Habitat",
            "$zufmscore:section": "event",

            "$zufmscore:repeat": true,
            "examples": [
                "Pantanal"
            ]
        },
        "eventRemarks": {
            "type": "string",
            "description": "comentários sobre o evento",
            "title": "Observações do Evento",
            "$zufmscore:section": "event",

            "examples": [
                "após das recentes chuvas o rio está quase na fase de inundação"
            ]
        },
        "fieldNotes": {
            "type": "string",
            "description": "pode ser um indicador da existencia de, b) uma referência a (publicação, link), ou c) texto das notas tomadas no campo sobre o evento",
            "title": "Anotações de Campo",
            "$zufmscore:section": "event",

            "examples": [
                "notas disponíveis na Biblioteca Central"
            ]
        },
        "measurementRemarks": {
            "type": "string",
            "description": "comentários ou notas que acompanham medições ou fatos; medidas concatenadas com \";\" informando unidade",
            "title": "Medidas",
            "$zufmscore:section": "measurementOrFact",

            "examples": [
                "peso (g): 23",
                "antebraço (mm): 47,1"
            ]
        },
        "continent": {
            "type": "string",
            "description": "apenas se for um divisão válida",
            "title": "Continente/Oceano",
            "$zufmscore:section": "location",
            "default": "América do Sul",
            "examples": [
                "América do Sul",
                "América do Norte",
                "Ásia"
            ]
        },
        "country": {
            "type": "string",
            "description": "nome do país",
            "title": "País",
            "$zufmscore:section": "location",
            "default": "Brasil",
            "$zufmscore:repeat": true,
            "examples": [
                "Brasil",
                "Paraguai",
                "Bolívia"
            ]
        },
        "countryCode": {
            "type": "string",
            "description": "codificação de acordo com ISO3166-1 e código alfa-3",
            "title": "Código do País",
            "$zufmscore:section": "location",
            "$zufmscore:dynamic": "countryCode",
            "examples": [
                "BRA",
                "PY",
                "PE"
            ]
        },
        "verbatimLocality": {
            "type": "string",
            "description": "utilizar quando se trata de organismos aquáticos",
            "title": "Bacia Hidrográfica",
            "$zufmscore:section": "location",
            "default": "Brasil",
            "$zufmscore:repeat": true,
            "examples": [
                "Bacia do Paraguai",
                "Bacia Hidrográfica Amazônica",
                "Bacia Hidrográfica do Paraná "
            ]
        },
        "stateProvince": {
            "type": "string",
            "description": "escrito por extenso, utilizar região quando são áreas de divisa entre estados",
            "title": "Estado/Província/Região",
            "$zufmscore:section": "location",
            "default": "Mato Grosso do Sul",
            "$zufmscore:repeat": true,
            "examples": [
                "Mato Grosso do Sul",
                "Paraná",
                "São Paulo"
            ]
        },
        "county": {
            "type": "string",
            "description": "o nome do município válido",
            "title": "Município",
            "$zufmscore:section": "location",

